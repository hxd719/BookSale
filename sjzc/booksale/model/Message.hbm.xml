<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="cn.sjzc.booksale.model.Message" table="t_message" >
		<id name="id">
			<generator class="native"/>
		</id>
		<property name="time"></property>
		<property name="state"></property>
		<many-to-one name="user" column="userid" lazy="false" ></many-to-one>
		<many-to-one name="sellInfo" column="sid" lazy="false" ></many-to-one>
		<many-to-one name="buyInfo" column="bid" lazy="false" ></many-to-one>
	</class>
	
</hibernate-mapping>