<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="cn.sjzc.booksale.model.User" table="t_user" >
		<id name="id">
			<generator class="native"/>
		</id>
		<property name="name"></property>
		<property name="password"></property>
		<property name="date"></property>
		<property name="expired"></property>
		<property name="sex"></property>
		<property name="phone"></property>
		<property name="token"></property>
		
		<set name="buyinfors"  cascade="all"  inverse="true" lazy="true">
			<key column="userid"></key>
        <one-to-many class="cn.sjzc.booksale.model.BuyInfor"/>
		</set>
		<set name="sellinfors"  cascade="all"  inverse="true" lazy="true">
			<key column="userid"></key>
        <one-to-many class="cn.sjzc.booksale.model.SellInfor"/>
		</set>
		
	</class>
	
</hibernate-mapping>