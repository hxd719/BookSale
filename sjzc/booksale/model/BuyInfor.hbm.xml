<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="cn.sjzc.booksale.model.BuyInfor" table="t_buyinfor" >
		<id name="id">
			<generator class="native"/>
		</id>
		<property name="bookName"></property>
		<property name="content"></property>
		<property name="publishTime"></property>
		<property name="deadline"></property>
		<many-to-one name="user" column="userid" lazy="false" ></many-to-one>
		<many-to-one name="book" column="bookid" lazy="false" ></many-to-one>
		<many-to-one name="category" column="categoryid" lazy="false"></many-to-one>
	</class>
	
</hibernate-mapping>